{
  "name": "cultuurnet/uitpas-beheer-silex",
  "description": "A silex application that serves as an intermediate between the UiTPAS API and front end apps.",
  "license": "GPL-3.0",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/2dotstwice/symfony2-coding-standard"
    },
    {
      "type": "vcs",
      "url": "https://github.com/2dotstwice/silex-cors-provider"
    }
  ],
  "require": {
    "cultuurnet/silex-uitid-provider": "dev-master",
    "cultuurnet/culturefeed-php": "dev-feature/UBR-250 as 1.5.2",
    "cultuurnet/search": "~1.2",
    "cultuurnet/calendar-summary": "~1.0",
    "cultuurnet/deserializer": "1.x-dev",
    "cultuurnet/culturefeed-http-guzzle": "~0.1",
    "deralex/yaml-config-service-provider": "~1.0",
    "doctrine/dbal": ">=2.2.0,<2.4.0-dev",
    "jdesrosiers/silex-cors-provider": "~0.1.5",
    "nicolopignatelli/valueobjects": "~3.0",
    "plasmaconduit/luhn":  "~0.1",
    "silex/silex": "~1.1",
    "symfony/browser-kit": "~2.3",
    "symfony/console": "~2.3",
    "symfony/config": "~2.3",
    "symfony/css-selector": "~2.3",
    "symfony/dom-crawler": "~2.3",
    "symfony/filesystem": "~2.3",
    "symfony/finder": "~2.3",
    "symfony/form": "~2.3",
    "symfony/locale": "~2.3",
    "symfony/process": "~2.3",
    "symfony/routing": "~2.6.9",
    "symfony/security": "~2.6",
    "symfony/serializer": "~2.3",
    "symfony/translation": "~2.3",
    "symfony/validator": "~2.3",
    "symfony/monolog-bridge": "~2.3",
    "symfony/twig-bridge": "~2.3",
    "swiftmailer/swiftmailer": "5.*",
    "cultuurnet/clock": "~0.1",
    "cultuurnet/hydra": "~0.1",
    "2dotstwice/collection": "~0.1"
  },
  "require-dev": {
    "escapestudios/symfony2-coding-standard": "dev-master as 2.0",
    "phpunit/phpunit": "~4.3",
    "squizlabs/php_codesniffer": "~2.2",
    "phing/phing": "~2.10",
    "satooshi/php-coveralls": "~0.7"
  },
  "autoload": {
    "psr-4": {
      "CultuurNet\\UiTPASBeheer\\": ["src", "app"]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CultuurNet\\UiTPASBeheer\\": ["test"]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
